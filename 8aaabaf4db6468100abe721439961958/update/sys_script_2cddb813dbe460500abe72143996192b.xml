        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	// Get order based on current number of resources
	var nextOrder = 0;
	var projResourceProjEstimate = new GlideRecord("x_icir_dealmaker_m2m_proj_resource_proj_estimate");
	projResourceProjEstimate.addQuery("project_estimate", current.project_estimate);
	projResourceProjEstimate.query();
	nextOrder = parseInt(projResourceProjEstimate.getRowCount() + 1) * 100;
	current.order = nextOrder;
	
	
	
	// Set onshore vs. offshore rate
	if (current.project_resource.type == "onshore")
		current.rate = 185;
	else
		current.rate = 45;

})(current, previous);]]></script>
